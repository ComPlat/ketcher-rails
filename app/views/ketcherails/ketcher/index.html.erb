<script>
  rnd.commontemplates = [];
  ui.commontemplate_tool_modes = [];

  var idx = 0;
  <% @template_categories.each do |cat| %>
    <% cat.common_templates.approved.order('name ASC').each do |tmpl| %>
      ui.commontemplate_cat_ids.push(<%= cat.id %>);
      ui.commontemplate_tool_modes.push('<%= "commontemplate#{cat.id}_" %>' + idx);
      rnd.commontemplates.push(<%= inline_template(tmpl) %>);
      idx++;
    <% end %>
  <% end %>

  <% @user_templates.each_with_index do |tmpl, idx| %>
    rnd.usertemplates.push(<%= inline_template(tmpl) %>);
    ui.usertemplate_tool_modes.push('usertemplate_' + <%= idx %>);
  <% end %>

  if(rnd.usertemplates.length > 0) {
    ui.DropdownListSetIconAndTitle($('usertemplate'), $('usertemplate_0'), false);
  };
</script>
